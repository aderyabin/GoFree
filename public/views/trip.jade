li.v-t-destination(data-index=index)
  .v-t-d-cities
    .v-t-d-inputwrap.m-input-suggest
      input.v-t-d-from_name.m-i-s-input(type="text", name='from_name', value=from_name)
      input.v-t-d-from_id(type="hidden", name='from_id', value=from_id)
      ul.m-i-s-list
    span.v-t-d-arrow
    .v-t-d-inputwrap.m-input-suggest
      input.v-t-d-to_name.m-i-s-input(type="text", name='to_name', value=to_name)
      input.v-t-d-to_id(type="hidden", name='to_name', value=to_name)
      ul.m-i-s-list
  .v-t-d-dates
    input.v-t-d-from(type="text")
    span.v-t-d-dash
    input.v-t-d-to(type="text")
